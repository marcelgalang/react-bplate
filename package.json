{
  "name": "react-bplate",
  "version": "1.0.0",
  "description": "Basic React Boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build-babel": "npm run build-babel-server && npm run build-babel-client",
    "build-babel-server": "babel src/server --out-dir ./dist",
    "build-babel-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/server.js",
    "test": "jest ./src",
    "clean": "rimraf dist node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcelgalang/react-bplate.git"
  },
  "keywords": [
    "Node",
    "React"
  ],
  "author": "Marcel Galang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marcelgalang/react-bplate/issues"
  },
  "homepage": "https://github.com/marcelgalang/react-bplate#readme",
  "dependencies": {
    "ajv": "^6.5.0",
    "express": "^4.16.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "webpack": "^4.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "react-test-renderer": "^16.3.2",
    "webpack-cli": "^2.0.15",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/enzyme.setup.js"
  }
}
